<?xml version="1.0" encoding="windows-1251"?>

<string_table>
	<string id="ui_mcm_dynamic_tonemap_extended_title">
		<text>Dynamic Time-based Tonemap Extended</text>
	</string>
	<string id="ui_mcm_menu_dynamic_tonemap_extended">
		<text>Dynamic Tonemap</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_enable_dynamic_tonemap">
		<text>Enable Dynamic Tonemap</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_adaptation">
		<text>Tonemap Adaptation</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_amount">
		<text>Tonemap Amount Change Rate</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_min_amount">
		<text>Minimal Allowed Tonemap Amount</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_max_amount">
		<text>Maximum Allowed Tonemap Amount</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_rain_amount_addition">
		<text>Amount Addition On Rain</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_rain_amount_addition_desc">
		<text>Result amount can go beyond maximum allowed tonemap but never more than 1</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_storm_amount_addition">
		<text>Amount Addition On Storm</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_storm_amount_addition_desc">
		<text>Result amount can go beyond maximum allowed tonemap but never more than 1</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_lowlum">
		<text>Tonemap Lowlum Change Rate</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_min_clear_lowlum">
		<text>Minimal Allowed Tonemap Lowlum On Clear Weather</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_min_not_clear_lowlum">
		<text>Minimal Allowed Tonemap Lowlum On Not Clear Weather</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_max_lowlum">
		<text>Maximum Allowed Tonemap Lowlum</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_rain_lowlum_reduction_coeff">
		<text>Minimal Tonemap Lowlum Reduction Coeff On Rain</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_rain_lowlum_reduction_coeff_desc">
		<text>Resulted lowlum would be min_not_clear_lowlum * rain_lowlum_reduction_coeff</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_storm_lowlum_reduction_coeff">
		<text>Minimal Tonemap Lowlum Reduction Coeff On Storm</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_storm_lowlum_reduction_coeff_desc">
		<text>Resulted lowlum would be min_not_clear_lowlum * storm_lowlum_reduction_coeff</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_middle">
		<text>Tonemap Middlegray Change Rate</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_min_middle_gray">
		<text>Minimal Allowed Tonemap Middlegray</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_max_middle_gray">
		<text>Maximum Allowed Tonemap Middlegray</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_lumscale_amb">
		<text>Lumscale Ambient Change Rate</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_lumscale_amb_addition">
		<text>Additional Rate of Lumscale Ambient</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_lumscale_amb_addition_desc">
		<text>Additional rate is applied between between 9 to 13 o'clock for increase and 19 to 24 o'clock for decrease</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_min_lumscale_amb">
		<text>Minimal Allowed Lumscale Ambient</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_max_lumscale_amb">
		<text>Maximum Allowed Lumscale Ambient</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_not_clear_amb_reduction_coeff">
		<text>Ambient Modifier On Not Clear Weather</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_not_clear_amb_reduction_coeff_desc">
		<text>Result would be lumscale_amb * not_clear_amb_reduction_coeff</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_rain_amb_reduction_coeff">
		<text>Ambient Modifier On Rain</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_rain_amb_reduction_coeff_desc">
		<text>Result would be lumscale_amb * rain_amb_reduction_coeff</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_storm_amb_reduction_coeff">
		<text>Ambient Modifier On Storm</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_storm_amb_reduction_coeff_desc">
		<text>Result would be lumscale_amb * storm_amb_reduction_coeff</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_lumscale_hemi">
		<text>Lumscale Hemi Change Rate</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_lumscale_hemi_addition">
		<text>Additional Rate of Lumscale Hemi</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_k_lumscale_hemi_addition_desc">
		<text>Additional rate is applied after 19 o'clock for compensating of excessive dark ground in evenings</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_min_lumscale_hemi">
		<text>Minimal Allowed Lumscale Hemi</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_max_lumscale_hemi">
		<text>Maximum Allowed Lumscale Hemi</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_not_clear_hemi_reduction_coeff">
		<text>Hemi Modifier On Not Clear Weather</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_not_clear_hemi_reduction_coeff_desc">
		<text>Result would be lumscale_hemi * not_clear_hemi_reduction_coeff</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_rain_hemi_reduction_coeff">
		<text>Hemi Modifier On Rain</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_rain_hemi_reduction_coeff_desc">
		<text>Result would be lumscale_hemi * rain_hemi_reduction_coeff</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_storm_hemi_reduction_coeff">
		<text>Hemi Modifier On Storm</text>
	</string>
	<string id="ui_mcm_dynamic_tonemap_extended_storm_hemi_reduction_coeff_desc">
		<text>Result would be lumscale_hemi * storm_hemi_reduction_coeff</text>
	</string>
</string_table>
